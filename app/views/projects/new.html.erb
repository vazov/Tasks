<h2>New Project</h2>
<%= form_for(@project) do |f| %>
<p>
<%= f.label :name %><br />
<%= f.text_field :name %>
</p>
<p>
<%= f.label :summary %><br />
<%= f.text_field :summary %>
</p>
<p>
<%= f.label :start_date %><br />
<%= f.text_field :start_date%>
</p>
<p>
<%= f.label :end_date %><br />
<%= f.text_field :end_date %>
<%= f.submit %>
<% end %>
